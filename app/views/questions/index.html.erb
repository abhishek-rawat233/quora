<% if notice %>
  <aside id="notice"><%= notice %></aside>
<% end %>

<% @questions.each do |question| %>
  <%= link_to user_question_path(@current_user, question) do %>
  <div class="question-container">
    <div class="question">
      <h3><strong>Q:</strong> <%= question.title %></h3>
      <% end %>
    </div>
    <h4><%= question.content %></h4>
    <%= link_to 'edit', edit_user_question_path(@current_user, question) %>
    <%= link_to 'delete', user_question_path(@current_user, question), method: :destroy %>
    <br>
    <br>
  </div>
<% end %>
